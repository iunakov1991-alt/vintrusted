{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=79200, must-revalidate"
        }
      ]
    }
  ],
  "functions": {
    "app/api/disable-deploy.js": {
      "runtime": "nodejs18.x"
    }
  }
}
